Parser(name, package, superclass, importTxt) ::= <<
parser grammar <name>Parser;

options {
	tokenVocab = <name>Lexer;
	<if(superclass)>superClass = <superclass>;<endif>
}

@header {
	package <package>;

	<if(importTxt)>import <importTxt>;<endif>
}

sampleSpec
	:	dottedName LBRACE
		statement*
		RBRACE
		EOF
	;

dottedName
	: name ( DOT name )*
	;

statement
	:	name COLON STRING_LITERAL SEMI 
	;

name 
	: ID
	;

>>
